public class Solution {
    public int JumpFloor(int target) {
        
        int fn2 = 1;
        int fn1 = 2;
        
        if(target==1)
            return 1;
        if(target==2)
            return 2;
        while(target >= 3){
            fn1 = fn1 + fn2;
            fn2 = fn1 - fn2;
            target--;
        }
        return fn1;
    }
}
